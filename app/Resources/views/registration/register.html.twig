{{ form_start(form, {'attr':{'data-ng-submit': 'submitForm($event)'}}) }}
    {{ form_row(form.username) }}
    {{ form_row(form.email) }}
    {{ form_row(form.plainPassword.first) }}
    {{ form_row(form.plainPassword.second) }}
    <button class="btn btn-primary" type="submit">Register</button>
{{ form_end(form) }}